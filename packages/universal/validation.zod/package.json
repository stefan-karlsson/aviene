    "eslint": "catalog:",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stefan-karlsson/aviene.git",
    "directory": "packages/universal/validation.zod"
  },
  "bugs": {
    "url": "https://github.com/stefan-karlsson/aviene/issues"
  },
