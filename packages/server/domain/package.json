{
  "name": "@aviene/domain",
  "version": "0.0.1",
  "description": "Domain building blocks",
  "author": "Stefan Karlsson",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "build": "tsc --emitDeclarationOnly --declaration",
    "check:types": "tsc --noEmit",
    "lint": "eslint ./src/*",
    "test": "node --import tsx --test __tests__/**/*.test.ts"
  },
  "sideEffects": false,
  "imports": {
    "#root/*": "./src/*",
    "#tests/*": "./__tests__/*"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "dependencies": {
    "@aviene/guards": "workspace:*",
    "@aviene/exceptions": "workspace:*",
    "@aviene/contracts": "workspace:*",
    "@aviene/contracts.runtime": "workspace:*",
    "@aviene/validation.zod": "workspace:*"
  },
  "devDependencies": {
    "@aviene/eslint.config": "workspace:*",
    "@aviene/typescript.config": "workspace:*",
    "@aviene/testing.nodejs": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "typescript": "catalog:",
    "tsx": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stefan-karlsson/aviene.git",
    "directory": "packages/server/domain"
  },
  "bugs": {
    "url": "https://github.com/stefan-karlsson/aviene/issues"
  },
  "keywords": [],
  "homepage": "https://github.com/stefan-karlsson/aviene",
  "files": [
    "dist/**"
  ]
}
