{
  "name": "@aviene/contracts.runtime",
  "version": "0.0.0",
  "description": "Commonly used contracts at runtime",
  "author": "Stefan Karlsson",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "build": "tsc --emitDeclarationOnly --declaration",
    "check:types": "tsc --noEmit",
