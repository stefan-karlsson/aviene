    ".": "./src/index.ts",
    "./type-guards/*": "./src/type-guards/*.ts"
  },
  "dependencies": {
    "@aviene/contracts": "workspace:*"
  },
  "devDependencies": {
    "@aviene/eslint.config": "workspace:*",
    "@aviene/typescript.config": "workspace:*",
    "eslint": "catalog:",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stefan-karlsson/aviene.git",
    "directory": "packages/shared/contracts.runtime"
  },
  "bugs": {
    "url": "https://github.com/stefan-karlsson/aviene/issues"
  },
  "keywords": [
    "type-guards",
    "contracts",
    "runtime"
  ],
  "homepage": "https://github.com/stefan-karlsson/aviene",
  "files": [
    "dist/**"
  ]
